    <rules>
        <fullName>Rattle_New Competitive Deal</fullName>
        <actions>
            <name>R_b101ab938fd743b1800f7b190ffee8e7</name>
            <type>OutboundMessage</type>
        </actions>
        <active>true</active>
        <formula>AND(NOT OR(ISBLANK(TEXT(Primary_Competitor__c)),ISBLANK(TEXT(Primary_Competitor__c))), ISCHANGED(Primary_Competitor__c), IsClosed = false, OR(ISBLANK(TEXT(Secondary_Competitor__c)),ISBLANK(TEXT(Secondary_Competitor__c))))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Rattle_New Customer for CSM Handoff</fullName>
        <actions>
            <name>R_43db64554a5448d08867996b0986b028</name>
            <type>OutboundMessage</type>
        </actions>
        <active>true</active>
        <formula>AND(ISCHANGED(StageName), OR(ISPICKVAL(StageName, &quot;Closed Won&quot;)), OR(CASESAFEID(RecordTypeId) = &quot;0125e000000Ak9EAAS&quot;))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
